{
  "name": "watchman",
  "version": "0.1.1",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo prebuild && expo run:android",
    "ios": "expo prebuild && expo run:ios",
    "web": "expo start --web",
    "format": "prettier --write app/**/*.tsx",
    "build:android:dev": "eas build --platform android --profile development",
    "build:ios:dev": "eas build --platform ios --profile development",
    "build:android:preview": "eas build --platform android --profile preview",
    "build:ios:preview": "eas build --platform ios --profile preview",
    "build:android:prod": "eas build --platform android --profile production",
    "build:ios:prod": "eas build --platform ios --profile production"
  },
  "dependencies": {
    "@bitcoin-js/tiny-secp256k1-asmjs": "github:abhiShandy/tiny-secp256k1-asmjs",
    "@types/react": "~18.2.14",
    "bip32": "github:abhiShandy/bip32",
    "bitcoinjs-lib": "github:abhiShandy/bitcoinjs-lib",
    "bs58check": "^3.0.1",
    "buffer": "^6.0.3",
    "expo": "^49.0.10",
    "expo-barcode-scanner": "~12.5.3",
    "expo-constants": "~14.4.2",
    "expo-linking": "~5.0.2",
    "expo-router": "2.0.0",
    "expo-splash-screen": "~0.20.5",
    "expo-sqlite": "~11.3.3",
    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "^2.4.0",
    "expo-updates": "~0.18.12",
    "react": "18.2.0",
    "react-native": "0.72.4",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "prettier": "^3.0.3"
  },
  "private": true,
  "resolutions": {
    "metro": "0.76.0",
    "metro-resolver": "0.76.0"
  }
}
